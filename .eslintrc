{
    "extends": [
        "airbnb",
        "react-app",
        "prettier",
        "prettier/react"
    ],
    "plugins": [
        "prettier"
    ],
    "env": {
        "es6": true,
        "browser": true
    },
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx", ".tsx"] }],
        "jsx-a11y/no-static-element-interactions": 0,
        "jsx-a11y/click-events-have-key-events": 0
    }
}
